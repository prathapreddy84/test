    </target>

    <!-- Retrieve an unpackaged set of metadata from your org -->
    <!-- The file unpackaged/package.xml lists what is to be retrieved -->
      <target name="upload">
      <!-- Upload the contents of the "mypkg" package -->
      <sf:deploy checkOnly="true" deployRoot="upload" maxPoll="${sf.maxPoll}" password="${sf.password}" runAllTests="false" serverurl="${sf.serverurl}" username="${sf.username}"/>     
    </target> 

    <!-- Retrieve all the items of a particular metadata type -->
    <target name="bulkRetrieve">
      <sf:bulkRetrieve maxPoll="${sf.maxPoll}" metadataType="${sf.metadataType}" password="${sf.password}" retrieveTarget="retrieveUnpackaged" serverurl="${sf.serverurl}" username="${sf.username}"/>
    </target>

    <!-- Retrieve metadata for all the packages specified under packageNames -->
    <target name="retrievePkg">
      <sf:retrieve maxPoll="${sf.maxPoll}" packageNames="${sf.pkgName}" password="${sf.password}" retrieveTarget="retrieveOutput" serverurl="${sf.serverurl}" username="${sf.username}"/>
    </target>

    <!-- Deploy the unpackaged set of metadata retrieved with retrieveUnpackaged -->
    <target name="deployUnpackaged">
      <sf:deploy deployRoot="retrieveUnpackaged" maxPoll="${sf.maxPoll}" password="${sf.password}" serverurl="${sf.serverurl}" username="${sf.username}"/>
    </target>

    <!-- Deploy a zip of metadata files to the org -->
    <target name="deployZip">
      <sf:deploy maxPoll="${sf.maxPoll}" password="${sf.password}" pollWaitMillis="1000" serverurl="${sf.serverurl}" username="${sf.username}" zipFile="${sf.zipFile}"/>
    </target>

    <!-- Shows deploying code & running tests for code in directory -->
    <target name="deployCode">
      <!-- Upload the contents of the "codepkg" directory, running the tests for just 1 class -->
      <sf:deploy deployRoot="codepkg" maxPoll="${sf.maxPoll}" password="${sf.password}" serverurl="${sf.serverurl}" username="${sf.username}">
        <runTest>SampleDeployClass</runTest>
      </sf:deploy>
    </target>

    <!-- Shows removing code; only succeeds if done after deployCode -->
    <target depends="proxy" name="undeployCode">
      <sf:deploy deployRoot="removecodepkg" maxPoll="${sf.maxPoll}" password="${sf.password}" serverurl="${sf.serverurl}" username="${sf.username}"/>
    </target>

    <!-- Shows retrieving code; only succeeds if done after deployCode -->
    <target depends="proxy" name="retrieveCode">
      <!-- Retrieve the contents listed in the file codepkg/package.xml into the codepkg directory -->
      <sf:retrieve maxPoll="${sf.maxPoll}" password="${sf.password}" retrieveTarget="codepkg" serverurl="${sf.serverurl}" unpackaged="codepkg/package.xml" username="${sf.username}"/>
    </target>

    <!-- Shows deploying code, running all tests, and running tests (1 of which fails), and logging. -->
    <target name="deployCodeFailingTest">
      <!-- Upload the contents of the "codepkg" package, running all tests -->
      <sf:deploy deployRoot="codepkg" logType="Debugonly" maxPoll="${sf.maxPoll}" password="${sf.password}" runAllTests="true" serverurl="${sf.serverurl}" username="${sf.username}"/>
    </target>

    <!-- Shows check only; never actually saves to the server -->
    <target name="deployCodeCheckOnly">
      <sf:deploy checkOnly="true" deployRoot="codepkg" maxPoll="${sf.maxPoll}" password="${sf.password}" serverurl="${sf.serverurl}" username="${sf.username}"/>
    </target>

