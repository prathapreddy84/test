	<!--<target name="retrieve">-->
      <!-- Upload the contents of the "mypkg" package -->
      <!--sf:deploy username="${sf.username}" password="${sf.password}" serverurl="${sf.serverurl}" maxPoll="${sf.maxPoll}" proxy="proxy" deployRoot="mypkg"/-->
